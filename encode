function feature() {
    var btnL = $(".f-btn-l"),
        btnR = $(".f-btn-r"),
        fIcon = $(".feature-icon-box a"),
        fText = $(".feature-txt-box a"),
        idx = 0,
        len = fIcon.length;
    btnL.on("click", function() {
        idx = idx-- <= 0 ? len - 1 : idx--;
        featureShow(idx);
        return false
    })
    btnR.on("click", function() {
        idx = idx++ >= len - 1 ? 0 : idx;
        featureShow(idx);
        return false
    });
    var featureShow = function(idx) {
        fIcon.eq(idx).addClass("on").siblings().removeClass("on");
        fText.eq(idx).addClass("on").siblings().removeClass("on");
    }
    var auto = setInterval(function() {
        idx = idx++ >= len - 1 ? 0 : idx;
        featureShow(idx);
    }, 5000);
    $(".feature").on("mouseenter", function() {
        clearInterval(auto);
    }).on("mouseleave", function() {
        auto = setInterval(function() {
            idx = idx++ >= len - 1 ? 0 : idx;
            featureShow(idx);
        }, 5000);
    })
}

function newTyep() {
    $(".news-type").each(function() {
        var _this = $(this);
        var txt = _this.text();
        switch (txt) {
            case '新闻':
                _this.addClass("news-type-1");
                break;
            case '公告':
                _this.addClass("news-type-2");
                break;
            case '活动':
                _this.addClass("news-type-3");
                break;
            case '媒体':
                _this.addClass("news-type-4");
                break;
            case '论坛':
                _this.addClass("news-type-5");
                break;
        }
    })
}

