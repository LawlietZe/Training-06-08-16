            pos: oDaTaNew14980
        };
        var l = ['<div id="bannerImg' + i.id + '"+ class="slider-img">'],
            s = ['<div class="slider-dot" id="bannerCtrl' + i.id + '">'],
            len = ai.sr.length,
            pre = i.pvg,
            u = 'http://ossweb-img.qq.com/upload/adw/';
        for (var i = 0; i < len; i++) {
            try {
                var c = ai.pos["pos" + ai.sr[i]];
                l.push('<a target="_blank" onclick="pgvSendClick({hottag:\'' + pre + (i + 1) + '\'});" href="' + c[1] + '" title="' + decodeURIComponent(c[0]) + '" ><img alt="" src="' + u + c[2] + '" width="' + c[3] + '" height="' + c[4] + '" \/></a>');
                s.push('<a class="dot-item" target="_blank" href="' + c[1] + '" onclick="pgvSendClick({hottag:\'' + pre + (i + 1) + '\'});">' + decodeURIComponent(c[0]) + '<\/a>');
            } catch (err) {
            }
        }
        ai.box.html(l.join("") + "<\/div>" + s.join("") + "<\/div>")
    };
    return function(ad) {
        for (var i = ad.length; i--;) {
            m(ad[i])
        }
    }
}();

function report(pids) {
    var s = [];
    s.push("flag1=" + pids[0] + "&flag2=" + pids[1] + "&flag3=" + pids[2]);
    for (var i = 0; i < timePoints.length; i++) {
        if (!!timePoints[i])
            s.push((i + 1) + "=" + (timePoints[i] - d0));
    }
    var url = "http://isdspeed.qq.com/cgi-bin/r.cgi?" + s.join("&");
    if (Math.random() < 0.3) {
        imgSendTimePoint = new Image();
        imgSendTimePoint.src = url;
    }
}

