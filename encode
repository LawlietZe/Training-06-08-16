//tab
$.fn.extend({
    "tab": function(options) {
        var defaults = {
            model: "click",
            hd: null,
            bd: null,
            hdActiveCls: 'active',
            arrow: false,
            arrowName: "arr",
            fade: false
        };
        var options = $.extend(defaults, options);
        return this.each(function() {
            var $el = $(this);
            $el.find(options.bd).not($el.find(options.bd).eq(0)).hide();
            $el.on(options.model, options.hd, function() {
                if ($(this).hasClass(options.hdActiveCls)) {
                    return false;
                }
                var idx = $(this).index();
                $(this).addClass(options.hdActiveCls).siblings().removeClass(options.hdActiveCls);
                if (options.fade == true) {
                    $el.find(options.bd).eq(idx).fadeIn().siblings(options.bd).hide();
                } else {
                    $el.find(options.bd).eq(idx).show().siblings(options.bd).hide();
                }
                if (options.arrow == true) {
                    var thisL = $(this).position().left;
                    var thisMW = $(this).outerWidth(true);
                   var thisW = $(this).innerWidth();
                    var arrowW = $(options.arrowName).outerWidth();
                    $(options.arrowName).stop().animate({
                      "left": thisL+(thisMW-thisW) -((arrowW - thisW) / 2 )
                    }, 200);
                }
                return false;
            });
        })
    }
});

function setOpacity(e, t) {
    e.filters ? e.style.filter = "alpha(opacity=" + t + ")" : e.style.opacity = t / 100
}

function fadeIn(e, t) {
    e.style.display = e.$oldDisplay || "", setOpacity(e, 0);
    for (var n = 0; 100 >= n; n += .5) ! function() {
        var a = n;
        setTimeout(function() {
            setOpacity(e, a)
        }, (a + 1) * t)
    }()
}

function loadroll(e, t) {
    var n = 8000,
        a = document.getElementById(e),
        i = document.getElementById(t),
        o = a.getElementsByTagName("a");
    for (var s = i.getElementsByTagName("a"), d = 0, r = setInterval(funSetime = function() {
                    d++,
                    d >= o.length && (d = 0),
                        l(d, 0)
                },
                n), l = function(e, t) {
                for (var n = (o[e].firstChild, s.length - 1); n >= 0; n--) s[n].className = "",
                    o[n].className = "s" == o[n].className ? "ss" : "h";
                s[e].className = "on",
                    1 == t ? fadeIn(o[e], 0) : fadeIn(o[e], 10),
                    o[e].className = "s"
            },
            c = s.length - 1; c >= 0; c--) o[c].cnt = s[c].cnt = c,
        o[c].onfocus = function() {
            this.blur()
        },
        o[c].onmouseover = function() {
            s[this.cnt].className = "on";
            clearInterval(r);
        },
        o[c].onmouseout = function() {
            s[this.cnt].className = "on";
            d = this.cnt,
                r = setInterval(funSetime = function() {
                        d++,
                        d >= o.length && (d = 0),
                            l(d, 0)
                    },
                    n)
        },
        s[c].onmouseover = function() {
            clearInterval(r),
                "on" != this.className && l(this.cnt, 1);
            s[this.cnt].className = "on"
        },
        s[c].onfocus = function() {
            this.blur()
        },
        s[c].onmouseout = function() {
            s[this.cnt].className = "on";
            d = this.cnt,
                r = setInterval(funSetime = function() {
                        d++,
                        d >= o.length && (d = 0),
                            l(d, 0)
                    },
                    n)
        };
    o[0].className = "s",
        s[0].className = "on"
}

